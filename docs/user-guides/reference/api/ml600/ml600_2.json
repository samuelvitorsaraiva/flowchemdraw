                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "boolean",
                                    "title": "Response Infuse Fake Device Left Pump Infuse Put"
                                }
                            }
                        }
                    },
